import java.util.Scanner;
class Human {
    String gender = "man";
    String name = "Ron";
    int height = 168;
    String eyes_color = "blue";
    String hair_color = "redhead";

    public Human() {
        this.gender = gender;
        this.height = height;
        this.name = name;
        this.eyes_color = eyes_color;
        this.hair_color = hair_color;
    }

    public void getInfo() {
        System.out.println("Object human. Gender: " + gender);
        System.out.println("Height: " + height);
        System.out.println("Name: " + name);
        System.out.println("Eyes color: " + eyes_color);
        System.out.println("Hair color: " + hair_color);
    }
}
    public class Main {
        public static void main(String[] args) {
            Scanner in = new Scanner(System.in);
            Human Human = new Human();
     Human.getInfo();
        }
    }
