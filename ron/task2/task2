import java.util.Scanner;
class animal {
    String name;
    static String type;
    int age;
    int weight;
    int height;
    public animal(){
     this.name = name;
     this.type = type;
     this.age = age;
     this.weight = weight;
     this.height = height;
    }
   public void getInfo(){
       System.out.println("Object animal. Type: " + type);
        System.out.println("Name: "+name);
        System.out.println("Age: " + age);
        System.out.println("Weight: " + weight);
        System.out.println("Height: " + height);
    }
}

    public class Main {
        public static void main(String[] args) {
            animal animal = new animal();
        animal.type = "кот";
        animal.name = "Тима";
        animal.age = 6;
        animal.weight = 5;
        animal.height = 30;
       animal.getInfo();
        }
    }
